<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="main" stylesheets="@style/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.assignMenuFighterController">
   <children>
      <Button fx:id="btnCancel" layoutX="411.0" layoutY="341.0" mnemonicParsing="false" onAction="#btnCancel_action" stylesheets="@style/style.css" text="Cancel">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="btnValidate" layoutX="487.0" layoutY="341.0" mnemonicParsing="false" onAction="#btnValidate_action" text="Validate">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font></Button>
      <TextField fx:id="searchBar" layoutX="36.0" layoutY="90.0" promptText="Search a fighter ..." styleClass="textfield" stylesheets="@style/textField.css" />
      <Text layoutX="124.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="font" text="Choose a Fighter to assign">
         <font>
            <Font name="Bernard MT Condensed" size="24.0" />
         </font>
      </Text>
      <TableView fx:id="tableItems" layoutX="140.0" layoutY="124.0" prefHeight="200.0" prefWidth="318.0" stylesheets="@style/tablesStyle.css">
        <columns>
          <TableColumn fx:id="cId" prefWidth="75.0" text="id" />
          <TableColumn fx:id="cModel" prefWidth="75.0" text="model" />
            <TableColumn fx:id="cInFlight" prefWidth="75.0" text="in Flight" />
            <TableColumn fx:id="cStatus" prefWidth="75.0" text="status" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
