<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="main" stylesheets="@style/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.creationMenuFighterController">
   <children>
      <VBox layoutX="152.0" layoutY="119.0" prefHeight="162.0" prefWidth="367.0" styleClass="smallfont" stylesheets="@style/style.css">
