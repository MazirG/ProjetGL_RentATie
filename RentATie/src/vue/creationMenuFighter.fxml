            <HBox prefHeight="30.0" prefWidth="100.0" styleClass="smallfont" stylesheets="@style/style.css">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="smallfont" text="Fighter ID :                     " />
                  <Label styleClass="labels" stylesheets="@style/style.css" text="{Default}" />
               </children>
            </HBox>
            <HBox prefHeight="39.0" prefWidth="373.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="smallfont" text="Fighter Model :              " />
                  <RadioButton fx:id="rbHunter" mnemonicParsing="false" onAction="#rbHunter_action" prefHeight="26.0" prefWidth="97.0" styleClass="labels" stylesheets="@style/style.css" text="Hunter" />
                  <RadioButton fx:id="rbXWing" mnemonicParsing="false" onAction="#rbXWing_action" prefHeight="26.0" prefWidth="106.0" styleClass="labels" stylesheets="@style/style.css" text="XWing" />
               </children>
            </HBox>
            <HBox prefHeight="38.0" prefWidth="377.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="smallfont" text="Fighter status :              " />
