               </children>
            </HBox>
            <HBox prefHeight="18.0" prefWidth="252.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="smallfont" text="Fighter in flight :   " wrappingWidth="164.4652099609375" />
                  <Label prefHeight="26.0" prefWidth="93.0" styleClass="labels" stylesheets="@style/style.css" text="No {Default}" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Text layoutX="54.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="font" text="Choose the model of the new Fighter">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Button fx:id="btnCancel" layoutX="454.0" layoutY="316.0" mnemonicParsing="false" onAction="#btnCancel_action" text="Cancel" />
      <Button fx:id="btnValidate" layoutX="336.0" layoutY="316.0" mnemonicParsing="false" onAction="#btnValidate_action" text="Validate" />
   </children>
</AnchorPane>
